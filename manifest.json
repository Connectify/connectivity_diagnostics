{
  "manifest_version": 2,
  "name": "Switchboard Connectivity Diagnostics",
  "version": "0.0.1",
  "minimum_chrome_version": "31",
  "offline_enabled": true,
  "default_locale": "en",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png"
  },
  "permissions" : [
   { "socket" : [
     "tcp-connect:*:443",
     "tcp-connect:*:80",
     "tcp-connect:*:25",
     "udp-bind:*",
     "udp-send-to:*:53",
     "udp-send-to:*:3478",
     "udp-send-to:*:19302"
   ]},
   "clipboardWrite",
   "experimental",
   "http://*.google.com/*",
   "https://*.google.com/*"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  }
}
